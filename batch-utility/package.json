{
  "name": "sovren-batch-utility",
  "description": "Sovren Batch Parser",
  "homepage": "https://sovren.com/",
  "author": "Sovren <support@sovren.com>",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod && electron .",
    "package-clean": "rimraf ./packages",
    "package-win": "ng build --prod && electron-builder --win",
    "package-mac": "ng build --prod && electron-builder --mac",
    "package-linux": "ng build --prod && electron-builder --linux",
    "package-wl": "electron-builder -wl"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.1",
    "@angular/common": "^6.0.1",
    "@angular/compiler": "^6.0.1",
    "@angular/core": "^6.0.1",
    "@angular/forms": "^6.0.1",
    "@angular/http": "^6.0.1",
    "@angular/platform-browser": "^6.0.1",
    "@angular/platform-browser-dynamic": "^6.0.1",
    "@angular/platform-server": "^6.0.1",
    "@angular/router": "^6.0.1",
    "@clr/angular": "^0.11.17",
    "@clr/icons": "^0.11.17",
    "@clr/ui": "^0.11.17",
    "@webcomponents/custom-elements": "^1.1.1",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.6",
    "electron-store": "^1.3.0",
    "electron-unhandled": "^1.1.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "n-readlines": "^1.0.0",
    "ngx-electron": "^1.0.4",
    "popper.js": "^1.14.3",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "winston": "^2.4.2",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "^6.0.1",
    "@angular/compiler-cli": "^6.0.1",
    "@angular/language-service": "^6.0.1",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.110",
    "codelyzer": "^4.3.0",
    "electron": "^1.8.6",
    "electron-builder": "^20.13.5",
    "electron-packager": "^12.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2"
  },
  "build": {
    "appId": "com.sovren.batchutility",
    "productName": "Sovren Batch Parser",
    "copyright": "Â© 2018 Copyright Sovren Group, Inc. All rights reserved",
    "artifactName": "${productName} Setup.${ext}",
    "publish": null,
    "directories": {
      "buildResources": "icons",
      "output": "packages",
      "app": "."
    },
    "files": [
      "dist/**/*",
      "icons/**/*",
      "main.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "icons/win/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "artifactName": "${productName}.${ext}",
      "category": "Utility"
    },
    "mac": {
      "icon": "icons/mac/icon.icns",
      "target": "dmg"
    }
  }
}
